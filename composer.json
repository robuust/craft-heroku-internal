{
    "version": "2.0.0",
    "name": "robuust/craft-heroku-internal",
    "description": "Module for internal use with Craft and Heroku",
    "require": {
        "php": "^7.0 || ^8.0",
        "craftcms/cms": "^4.0",
        "craftcms/aws-s3": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "robuust\\heroku\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
